{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Workers": {
    "UserInfoReadWorker": {
      "Enabled": true,
      "BulkSize": 5000,
      "Schedule": "0 2 * * *"
    },
    "UserInfoDownloadWorker": {
      "Enabled": true,
      "BulkSize": 5000,
      "Schedule": "0 1 * * *"
    },
    "UserPhotoWorker": {
      "Enabled": true,
      "BulkSize": 5000,
      "Schedule": "0 3 * * *"
    },
    "UserInfoDismissReadWorker": {
      "Enabled": true,
      "BulkSize": 5000,
      "Schedule": "0 4 * * *"
    }
  },
  "AllowedHosts": "*"
}
